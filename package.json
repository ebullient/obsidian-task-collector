{
  "name": "obsidian-task-collector",
  "version": "0.7.2",
  "private": true,
  "description": "Manage completed tasks within a document in Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "pretest": "eslint --ignore-path .gitignore src/",
    "test": "jest",
    "prettier": "prettier -w 'src/**/*.ts'",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "test-version": "auto-changelog -p",
    "brat-notes": "run() { auto-changelog --stdout --hide-credit --hide-empty-releases --template .github/changelog.hbs -v $1 --starting-version $1  > release-notes.md; }; run"
  },
  "keywords": [
    "obsidian",
    "obsidian-md",
    "obsidian-plugin",
    "obsidian-md-plugin"
  ],
  "author": "ebullient",
  "repository": "github.com:ebullient/obsidian-task-collector",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.17.19",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "auto-changelog": "^2.3.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "esbuild-copy-plugin": "^1.0.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "moment": "^2.29.1",
    "obsidian": "^0.12.17",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "4.4.4"
  },
  "auto-changelog": {
    "backfillLimit": false,
    "commitLimit": false,
    "ignoreCommitPattern": "(changelog|release|ðŸ”¨ brat release|Update README).*"
  }
}
